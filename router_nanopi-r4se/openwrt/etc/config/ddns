
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'kylewesthaus'
	option enabled '1'
	option interface 'in_wan'
	option service_name 'namecheap.com'
	option lookup_host 'kylewesthaus.com'
	option domain 'kylewesthaus.com'
	option username '@'
	option password '<REDACTED:kylewesthaus>'
	option use_ipv6 '0'
	option ip_source 'network'
	option ip_network 'in_wan'
	option use_https '1'
	option cacert '/etc/ssl/certs'
	option use_syslog '2'

