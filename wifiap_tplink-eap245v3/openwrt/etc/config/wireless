
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0'
	option country 'US'
	option band '5g'
	option htmode 'VHT40'
	option channel '108'
	option cell_density '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
	option country 'US'
	option band '2g'
	option htmode 'HT20'
	option channel 'auto'
	option cell_density '0'


config wifi-iface 'wifinet1'
	option device 'radio0'
	option mode 'ap'
	option ssid 'HackerFi-Trusted'
	option encryption 'sae'
	option key '<REDACTED:trusted>'
	option wpa_disable_eapol_key_retries '1'
	option network 'in_trusted'

config wifi-iface 'wifinet2'
	option device 'radio1'
	option mode 'ap'
	option ssid 'HackerFi-Guest'
	option encryption 'sae'
	option key '<REDACTED:guest>'
	option wpa_disable_eapol_key_retries '1'
	option isolate '1'
	option network 'in_guest'

config wifi-iface 'wifinet3'
	option device 'radio1'
	option mode 'ap'
	option ssid 'HackerFi-IoT'
	option encryption 'psk2+ccmp'
	option key '<REDACTED:iot>'
	option isolate '1'
	option network 'in_iot'

