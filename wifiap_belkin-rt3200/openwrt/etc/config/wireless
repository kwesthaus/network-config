
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/18000000.wmac'
	option country 'US'
	option band '2g'
	option htmode 'HT20'
	option channel 'auto'

config wifi-iface 'wifi_iot'
	option device 'radio0'
	option network 'in_iot'
	option mode 'ap'
	option ssid 'HackerFi-IoT'
	option encryption 'psk2+ccmp'
	option key '<REDACTED:iot>'
	option isolate '1'


config wifi-device 'radio1'
	option type 'mac80211'
	option path '1a143000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option country 'US'
	option band '5g'
	option htmode 'VHT40'
	option channel '108'

config wifi-iface 'wifi_trusted'
	option device 'radio1'
	option network 'in_trusted'
	option mode 'ap'
	option ssid 'HackerFi-Trusted'
	option encryption 'sae'
	option key '<REDACTED:trusted>'
	option wpa_disable_eapol_key_retries '1'

config wifi-iface 'wifi_guest'
	option device 'radio1'
	option network 'in_guest'
	option mode 'ap'
	option ssid 'HackerFi-Guest'
	option encryption 'sae'
	option key '<REDACTED:guest>'
	option wpa_disable_eapol_key_retries '1'
	option isolate '1'

